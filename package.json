{
  "name": "cmad-sub",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "concat:mocks": "node \"./mocks/concat-json.js\"",
    "serve:mocks": "json-server --watch mocks/data.json --middlewares mocks/singular.js",
    "serve:mocks:en": "json-server --watch mocks/data_en.json --middlewares mocks/singular.js",
    "serve:mocks:local": "json-server --host 0.0.0.0 mocks/data.json --middlewares mocks/singular.js",
    "mock": "concurrently \"npm run concat:mocks\" \"npm run serve:mocks\"",
    "mock:en": "concurrently \"npm run concat:mocks\" \"npm run serve:mocks:en\"",
    "mock:local": "concurrently \"npm run concat:mocks\" \"npm run serve:mocks:local\"",
    "serve": "concurrently \"npm run mock\" \"ng serve\"",
    "serve:en": "concurrently \"npm run mock:en\" \"ng serve -c en\"",
    "serve:local": "concurrently \"npm run mock:local\" \"ng serve --host 0.0.0.0\"",
    "serve:prod": "concurrently \"npm run mock:local\" \"ng serve --prod --host 0.0.0.0\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/material-moment-adapter": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "core-js": "^2.6.8",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.48",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "glob-watcher": "^5.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-concat": "0.0.1",
    "json-server": "^0.14.2",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
